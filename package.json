{
  "name": "person-service",
  "version": "1.0.0",
  "description": "Simple REST service that serves a person collection",
  "main": " dist/app.js",
  "scripts": {
    "start": "npm run build && npx node  $NODE_DEBUG_OPTION  dist/app.js",
    "build": "npx babel src/ -d dist/",
    "prepublish": "npm run build",
    "flow": "npx flow",
    "docker-build": "docker build . -t mauget/person-service:latest",
    "docker-push": "docker push mauget/person-service:latest",
    "docker-start": "docker run --name person-service --rm -p 3000:3000 mauget/person-service:latest",
    "docker-stop": "docker stop person-service",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "REST",
    "service",
    "demo"
  ],
  "author": "Louis E. Mauget",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.2",
    "@babel/preset-flow": "^7.0.0",
    "flow-bin": "^0.89.0"
  }
}
